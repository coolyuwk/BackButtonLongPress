    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:BackButtonLongPress"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <ShellContent Title="MainPage" ContentTemplate="{DataTemplate local:MainPage}">
        </ShellContent>
        <ShellContent Title="NewPage" ContentTemplate="{DataTemplate local:NewPage}">
        </ShellContent>
    </TabBar>
</Shell>

